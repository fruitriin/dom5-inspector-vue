{
  "name": "dom5-inspector",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "public/index.html ;vue-cli-service serve",
    "build": "cp -f public/index.build.html public/index.html; vue-cli-service build; cp -f public/index.dev.html public/index.html; ",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "@vue/eslint-config-prettier": "^3.0.0-rc.10",
    "axios": "^0.18.0",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-cli-plugin-axios": "^0.0.4",
    "vue-cli-plugin-bootstrap-vue": "^0.1.0",
    "vue-template-compiler": "^2.5.16"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
